      <parameter name="className" value="it.eng.spagobi.sdk.test.stub.TestConnectionServiceSoapBindingSkeleton"/>
      <parameter name="wsdlPortType" value="TestConnectionService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
